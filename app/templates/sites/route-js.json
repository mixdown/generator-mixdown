{
  "overlay": true,
  "plugins": {
    "router": {
      "options": {
        "routes": {
          "js": {
            "method": "GET",
            "name": "js",
            "path": "/js/:js_src",
            "description": "JS resources",
            "handler": "js",
            "params": {
              "js_src": {
                "kind": "rest",
                "regex": "(.*\\.js)",
                "default": "",
                "enabled": true
              }
            },
            "enabled": true
          }
        }
      }
    }
  }
}
